<!-- DETALLE DE LOS MOVIMIENTOS -->
<div id="modDetM" class="modal fade" role="dialog">
  <div class="modal-dialog modal-lg">

    <!-- Modal content-->
    <div class="modal-content">
        <div class="modal-header" style="background-color:#8A0829;">
          <button type="button" class="close" data-dismiss="modal" style="color:white;">&times;</button>
          <h3 class="modal-title" style="color:#FFFFFF;"><b>Detalle del Movimiento</b></h3>
        </div>
      <div class="modal-body" style="height: 260px; ">
      		<form  role="form">
	        	<div class="form-group">
	        		<div class="col-xs-12">
	        			<em>ARTICULO</em>
	        		</div>
	        		<div class="col-xs-12">
	        			<label id="articuloMos"></label> (<label id="medidaMos"></label>)
	        		</div>
	        	</div>
	        	<div class="form-group">
	        		<div class="col-xs-12">
		        		<em>CANTIDAD RETIRADA</em>
		        	</div>
		        	<div class="col-xs-12">
		        		<em><label id="cantidadMos"></label></em>
		        	</div>
	        	</div>
	        	<div class="form-group">
	        		<div class="col-xs-12">
			        	<em>FECHA DE RETIRO</em>
			        </div>
			       <div class="col-xs-12">
			       		<label id="fechaMos"></label>	
			       </div>  
	        	</div>
	        	<div class="form-group">
	        		 <div class="col-xs-12">
	        		 	<em>OBSERVACION</em>
	        		 </div>
	        		 <div class="col-xs-12">
	        		 	<label id="obsMos"></label>
	        		 </div>
	        	</div>
        	</form>
        	<p></p>
      </div>
    </div>
  </div>
</div>


